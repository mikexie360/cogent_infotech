package Four;

public class FourMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Storage st = new Storage(0);
		Counter ct = new Counter(st);
		Printer pt = new Printer(st);
		
		ct.start();
		pt.start();
		synchronized(ct, pt) {
			for(int i = st.storedInt; i<100; i++) {
			
			}
		}
	}

}
