
public class SportsCar extends Car{
	
	
	
	SportsCar(int speed, int noOfGear) {
		super(speed, noOfGear);
	}
	
	public void AirBalloonType() {
		System.out.println("Speed " + speed);
		System.out.println("noOfGear "+ noOfGear);	
	}
	
	@Override
	public void display() {
		System.out.println("Speed " + speed);
		System.out.println("noOfGear "+ noOfGear);
	}	

}