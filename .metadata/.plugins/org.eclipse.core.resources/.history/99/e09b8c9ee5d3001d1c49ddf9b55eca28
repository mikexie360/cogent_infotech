package cogent.infotech.com.autowiring.annotation;

import org.springframework.beans.factory.annotation.Autowired;

public class Person {
	
	@Autowired
	private Car car;

	public Person(Car car) {
		this.car = car;
	}
	
	
	public Car getCar() {
		return car;
	}
	@Autowired
	public void setCar(Car car) {
		this.car = car;
	}
	
	
	
	
}
