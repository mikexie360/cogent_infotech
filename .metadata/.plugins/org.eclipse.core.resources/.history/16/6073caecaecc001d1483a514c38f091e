package pkg;

public class WrapperDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		byte a = 0;
		short b = 0;
		int c = 0;
		long d = 0;
		float e = 0;
		double f = 0;
		char g = 'A';
		boolean h = false;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
		System.out.println(h);
		
		Byte a1 = new Byte(a);
		Short b1 = new Short(b);
		Integer c1 = new Integer(c);
		Long d1 = new Long(d);
		Float e1 = new Float(e);
		Double f1 = new Double(f);
		Character g1 = new Character(g);
		Boolean h1 = new Boolean(h);

		System.out.println(a1);
		System.out.println(b1);
		System.out.println(c1);
		System.out.println(d1);
		System.out.println(e1);
		System.out.println(f1);
		System.out.println(g1);
		System.out.println(h1);
		
		byte a2 = a1.byteValue();
		Short b2 = b1.shortValue();
		Integer c2 = c1.intValue();
		Long d2 = d2.longValue();
		Float e2 = e2.floatValue();
		Double f2 = f2.doubleValue();
		Character g2 = g2.charValue();
		Boolean h2 = h2.booleanValue()
	}

}
