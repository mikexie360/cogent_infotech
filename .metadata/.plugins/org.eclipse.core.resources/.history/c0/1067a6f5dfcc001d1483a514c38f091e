
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			Emp e = new Emp(1,"mike","Manager",1000,1000);
			e.printDET();
		} catch (LowSalExpectation e) {
			System.out.println("Low Salary Expectation\n");
		}
		
		try {
			Emp e = new Emp(1,"mike","Officer",1000,1000);
			e.printDET();
		} catch (LowSalExpectation e) {
			System.out.println("Low Salary Expectation\n");
		}
		
		try {
			Emp e = new Emp(1,"mike","Clerk",100,1000);
			e.printDET();
		} catch (LowSalExpectation e) {
			System.out.println("Low Salary Expectation\n");
		}
	}

}
